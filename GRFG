#include<stdio.h>
#include<string.h>

int main()
{
	int n=0;
	scanf("%d",&n);
	int ja[9],ba[9];
	char id[9][17];
	for(int i=0;i<n;i++)
	{
		scanf("%s %d %d",id[i],&ja[i],&ba[i]);
	}

	int m=1;
	scanf("%d",&m);
	int s[6];
	for(;i<m;i++) scanf("%d",&s[6]);

	for(;i<m;i++)
	{
		int index;
		for(int j=0;j<n;j++)
		{
			if(s[i]==ja[j])
			{
				index=j;
				break;
			}
		}
		printf("%s %d\n",id[index],ba[index]);
	}
	return 0;
}
